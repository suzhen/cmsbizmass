<h1>Listing enterprises</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Province</th>
    <th>City</th>
    <th>Store name</th>
    <th>Address</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th>Tel</th>
    <th>Kind</th>
    <th>Remark</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @enterprises.each do |enterprise| %>
  <tr>
    <td><%= enterprise.name %></td>
    <td><%= enterprise.province %></td>
    <td><%= enterprise.city %></td>
    <td><%= enterprise.store_name %></td>
    <td><%= enterprise.address %></td>
    <td><%= enterprise.longitude %></td>
    <td><%= enterprise.latitude %></td>
    <td><%= enterprise.tel %></td>
    <td><%= enterprise.kind %></td>
    <td><%= enterprise.remark %></td>
    <td><%= link_to 'Show', enterprise %></td>
    <td><%= link_to 'Edit', edit_enterprise_path(enterprise) %></td>
    <td><%= link_to 'Destroy', enterprise, :confirm=> 'Are you sure?', :method=> :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Enterprise', new_enterprise_path %>
