= simple_form_for(@ad_space,:html=>{:class=>'form-horizontal'}) do |f|
  = f.error_notification

  .form-inputs
    = f.input :name
    = f.input :symbol
    = f.input :kind
    = f.input :path
    = f.input :width
    = f.input :height
    = f.input :setting
    = f.input :description
    = f.input :items
    = f.input :disabled

  .form-actions
    = f.button :submit
